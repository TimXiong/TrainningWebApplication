
@model IEnumerable<TrainningData.Product>

<table class="table-striped">
	@foreach (var product in Model)
	{
		<tr>
			<td><a asp-controller="Product" asp-action="Detail" asp-route-id="@product.Id">@product.ProductName</a></td>
			@*<td>@product.ProductName</td>*@
			<td>@product.UnitPrice</td>
		</tr>
	}
</table>